      type: int_32!
      description: Number of affected rows
    - name: returning
      type: "[sars!]!"
      description: Objects that were affected by insert
  graphql:
    typeName: insert_sars_result
    inputTypeName: insert_sars_result_input
  dataConnectorTypeMapping:
    - dataConnectorName: aml_postgres
      dataConnectorObjectType: insert_sars_result

---
kind: TypePermissions
version: v1
definition:
  typeName: insert_sars_result
  permissions:
    - role: admin
      output:
        allowedFields:
          - affected_rows
          - returning

---
kind: Command
version: v1
definition:
  name: insert_sars
  outputType: insert_sars_result!
  arguments:
    - name: objects
      type: "[sars_insert_input!]!"
      description: Objects to insert
    - name: returning
      type: "[string!]"
      description: Fields to return
  source:
    dataConnectorName: aml_postgres
    dataConnectorCommand:
      procedure: insert_sars
  graphql:
    rootFieldName: insert_sars
    rootFieldKind: Mutation
  description: Insert into sars

---
kind: CommandPermissions
version: v1
