---
kind: ObjectType
version: v1
definition:
  name: sars_insert_input
  description: Input type for inserting into sars
  fields:
    - name: customer_id
      type: int_32
      description: New value for sars.customer_id
    - name: filed_date
      type: timestamp
      description: New value for sars.filed_date
    - name: reason
      type: string
      description: New value for sars.reason
    - name: sar_id
      type: int_32!
      description: New value for sars.sar_id
    - name: status
      type: string
      description: New value for sars.status
    - name: transaction_id
      type: int_32
      description: New value for sars.transaction_id
  graphql:
    typeName: sars_insert_input
    inputTypeName: sars_insert_input_input
  dataConnectorTypeMapping:
    - dataConnectorName: aml_postgres
      dataConnectorObjectType: sars_insert_input

---
kind: TypePermissions
version: v1
definition:
  typeName: sars_insert_input
  permissions:
    - role: admin
      output:
        allowedFields:
          - customer_id
          - filed_date
          - reason
          - sar_id
          - status
          - transaction_id

---
kind: ObjectType
version: v1
definition:
  name: insert_sars_result
  description: insert from sars result
  fields:
